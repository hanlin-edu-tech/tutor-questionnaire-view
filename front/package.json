{
  "name": "svelte-app",
  "version": "1.0.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.4",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.1.2"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.7.0",
    "gulp": "^4.0.2",
    "path": "^0.12.7",
    "sirv-cli": "^0.4.4"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "publishTest": "rollup -c && gulp uploadGcsTest",
    "publishProd": "rollup -c && gulp uploadGcsProd"
  },
  "jshintConfig": {
    "esversion": 8,
    "devel": true,
    "undef": true,
    "globals": {
      "openModal": true
    },
    "browser": true,
    "eqeqeq": true
  }
}
